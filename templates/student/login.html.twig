{#{% block title %}dashboard{% endblock %}#}

{% block body %}
     <div class="header">
        <a href="#default" class="logo">CompanyLogo</a>
        <div class="header-right">

            <a class="active" href="#home">Home</a>
            <a href="#contact">find Tutor</a>
            <a href="#contact">You Subjects</a>
            <a href="#about">About</a>
            <a href="#about">Help</a>
            <a href="{{ path('student_home') }}">Logout</a>

        </div>
    </div>
<div class="container emp-profile">
    <form method="post">
        <div class="row">
            <div class="col-md-4">
            </div>
            <div class="col-md-12">
            </div>
            {% if app.user %}

            <div class="col-md-2">
                <a  href="{{ path('update_student',{'id': data.id}) }}">Edit Profile</a><br>
             </div>
            {% endif %}


        </div>

        <div class="row">
            <div class="col-md-4">
                <div class="profile-head">
                    {% for message in app.flashes('success') %}
                        <div class="alert alert-success">
                            {{ message }}
                        </div>
                    {% endfor %}
                     <p class="proile-rating">Name: <span>{{ data.user.username }}</span></p>
                     <p class="proile-rating"> phone No: <span>{{ data.phone }}</span></p>
                    <p class="proile-rating" >Age : <span> {{ data.age }}</span></p>
                    <p class="proile-rating" >Gender : <span> {{ data.gender }}</span></p>
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                    </ul>
                </div>
                <div class="profile-work">
                    <p>program: <span>{{ data.program }}</span></p>
                </div>
            </div>

        </div>
    </form>
</div>





{% endblock %}

